{
  "name": "star-burger",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "parcel watch bundles-src/index.js -d bundles --public-url /static/",
    "build": "parcel build bundles-src/index.js -d bundles --public-url /static/"
  },
  "browserslist": [
    "last 2 chrome versions"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "core-js": "^3.21.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-bootstrap": "^0.33.1",
    "react-dom": "^17.0.2",
    "react-transition-group": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "parcel-bundler": "1.12.5",
    "sass": "^1.32.8"
  }
}
